{
    "name": "loubal70/livewire-wizard",
    "description": "Laravel Livewire Wizard (Multi-Step Form) component",
    "keywords": [
        "laravel",
        "livewire",
        "multi-steps",
        "wizard",
        "form"
    ],
    "homepage": "https://github.com/loubal70/livewire-wizard",
    "license": "MIT",
    "authors": [
        {
            "name": "Louis Boulanger",
            "email": "louis.boulanger.work@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "livewire/livewire": "^2.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "loubal70\\LivewireWizard\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "loubal70\\LivewireWizard\\LivewireWizardServiceProvider"
            ],
            "aliases": {
                "LivewireWizard": "loubal70\\LivewireWizard\\Facades\\LivewireWizard"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
